      <main role="main" class="main-content">
        <div class="container-fluid">
          <div class="row justify-content-center">
            <div class="col-11">
              <h2 class="page-title">Usuarios</h2>
              <div class="card shadow mb-4">
                <div class="card-header">
                  <strong class="card-title">Orinoco de la Cuenca</strong>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-6">
                      <form id="formulariousuario" action="">
                        <div class="form-group mb-3">
                          <label for="simpleinput">Tipo de Documento</label>
                          <select name="" id="tipo" class="form-control">
                            <option value="">Cedula</option>
                            <option value="">Cedula extranjeria</option>
                            <option value="">Tarjeta Identidad</option>
                          </select>
                        </div>
                        <div class="form-group mb-3">
                          <label for="documento">Numero Documento</label>
                          <input type="number" id="numero" class="form-control">
                        </div>
                        <div class="form-group mb-3">
                          <label for="example-password">Nombre Completo</label>
                          <input type="text" id="nombre" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group mb-3">
                          <label for="example-email">Correo Electronico</label>
                          <input type="email" id="correo" name="example-email" class="form-control" autocomplete="off">
                        </div>
                    </div> <!-- /.col -->
                    <div class="col-md-6">
                      <div class="form-group mb-2">
                        <label>Telefono</label>
                        <input type="number" id="telefono" class="form-control" autocomplete="off">
                      </div>
                      <div class="form-group mb-3">
                        <label for="example-password">Contraseña</label>
                        <input type="password" id="pass" class="form-control" onclick="mostrarAlerta()"><br>
                        <label for="example-password">Confirmar Contraseña</label>
                        <input type="password" id="confirmar" class="form-control"><br>
                        <label for="simpleinput">Asignar Rol</label>
                        <select name="" id="simpleinput" class="form-control">
                          <option value="">Administrador</option>
                          <option value="">Residente</option>
                          <option value="">Vigilante</option>
                        </select>
                      </div>

                    </div>
                    <div class="card-body">
                      <button id="btn-usuario" type="submit" class="btn mb-2 btn-outline-primary btn-block"><span
                          class="fe fe-arrow-right fe-16 mr-2"></span>Crear</button>
                      <button type="button" class="btn mb-2 btn-outline btn-block"><span
                          class="fe fe-arrow-left fe-16 mr-2"></span>Regresar</button>
                    </div>
                  </div>
                  </form>
                </div>
                <p id="registrousuario" style="text-align: center; color:black"><strong></strong></p><br>
